import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
    Scanner ler = new Scanner(System.in);	    
		int caso;
		int n, m, resultado;
		
		System.out.println("Primero informe dois números");
		n=ler.nextInt();
		m=ler.nextInt();
		
		System.out.println("Agora escolha qual das operações você deseja");
		System.out.println("[1] Adição");
		System.out.println("[2] Subtração");
		System.out.println("[3] Multiplicação");
		System.out.println("[4] Divisão");
		caso=ler.nextInt();
		
		switch (caso) {
		    case 1:
		        resultado = n+m;
		        System.out.println("O resultado foi de: "+resultado);
		        break;
	        case 2:
		        resultado = n-m;
		        System.out.println("O resultado foi de: "+resultado);
		        break;
		    case 3:
		        resultado = n*m;
		        System.out.println("O resultado foi de: "+resultado);
		        break;
		    case 4:
                resultado = n/m;
		        System.out.println("O resultado foi de: "+resultado);
		        break;	     
		   default:
		   System.out.println("Numero infomado não encontrado");
		   break;
		}
	}
}
