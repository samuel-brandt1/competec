import java.util.Scanner;
public class Ex01 {
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int vezes = 1;
        String palavra;
        
        System.out.println("Escreva algo:");
        palavra = sc.nextLine();
        while (vezes <=5) {
            System.out.println(vezes+" Â° "+palavra);
            vezes++;
        }
    }
}
