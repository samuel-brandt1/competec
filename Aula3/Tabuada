import java.util.Scanner;
public class Main 
{
    public static void main (String[] args) {
        Scanner sc = new Scanner(System.in);
        int num, total;
        
        System.out.println("Digite um n√∫mero:");
        num = sc.nextInt();
        
        for (int i = 0; i<=10; i++) {
             total = i*num; 
            System.out.println(num+" x "+i+" = "+total);
        }
    }
}
