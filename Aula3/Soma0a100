public class Soma0a100 {
    public static void main (String[] args) {
        int cont = 0, soma = 0;
        System.out.println("Soma dos n√∫meros de 0 A 100");
        
        while (cont <= 100) {
            soma = soma + cont;
            cont++;
        }
        System.out.println("O resultado deu: "+soma);
    }
}
