import java.util.Scanner;
public class AcerteASenha
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String senha = "BATATA";
		
		System.out.println("Descubra a senha secreta");
		String tentativa = sc.nextLine();
		String taCerto = (tentativa.equalsIgnoreCase(senha)) ? "Parabéns, você acertou!" : "Errou, que pena!";
		System.out.println(taCerto);
	}
}
